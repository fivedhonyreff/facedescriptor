    "version": "0.2.0",
    "configurations": [
        {
            "name": "mpia-development",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "development",
                "-t",
                "lib/_main_flavor/main_development.dart"
            ]
        },
        {
            "name": "mpia-staging",
            "request": "launch",
            "type": "dart",
            "args": [
                "--flavor",
                "staging",
                "-t",
                "lib/_main_flavor/main_staging.dart"
            ]
        },
        {
            "name": "mpia-production",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "args": [
                "--flavor",
                "prod",
                "-t",
                "lib/_main_flavor/main_prod.dart"
            ]
        }
    ]
}
